    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@oclif/core": "^4.5.2",
    "@oclif/plugin-help": "^6.2.32",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "toml": "^3.0.0",
    "xml2js": "^0.6.2",
